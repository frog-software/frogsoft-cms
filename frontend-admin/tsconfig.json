{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "baseUrl": "./",
    "skipLibCheck": true,
    "incremental": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "allowJs": true,
    "paths": {
      "components": [
        "src/components"
      ],
      "components/*": [
        "src/components/*"
      ],
      "services": [
        "src/services"
      ],
      "services/*": [
        "src/services/*"
      ],
      "consts": [
        "src/consts"
      ],
      "consts/*": [
        "src/consts/*"
      ],
      "utils": [
        "src/utils"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "types": [
        "src/types"
      ],
      "types/*": [
        "src/types/*"
      ]
    }
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}